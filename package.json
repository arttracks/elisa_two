{
  "name": "elysatwo",
  "description": "Elysa 2.0",
  "version": "1.0.0",
  "author": "David Newbury <david.newbury@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --watch & bundle exec puma -C config/puma.rb",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/fontawesome-free-solid": "^5.0.4",
    "@fortawesome/vue-fontawesome": "0.0.22",
    "axios": "^0.17.1",
    "bulma": "^0.6.2",
    "bulma-extensions": "^0.6.1",
    "vue": "^2.5.9",
    "vuex": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "mapCoverage": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jest": "^22.0.6",
    "node-sass": "^4.5.3",
